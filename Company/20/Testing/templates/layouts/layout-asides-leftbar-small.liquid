{% include header %}
<!-- The following CSS are included as plugins and can be removed if unused-->
{% block styles %}{% endblock %}
</head>
<body class="infobar-offcanvas leftbar-small layout-overflow">
  {% include analytics %}
  {% include headerbar %}
  {% include topnav %}
  <div id="wrapper">
    <div id="layout-static">
      <div class="static-leftbar-wrapper leftbar-default">
        <div class="static-leftbar">
          {% include leftbar %}
        </div>
      </div>
      <div class="static-content-wrapper">
        <div class="static-content m-n">
          <div class="page-content aside-content" style="overflow-y: auto; overflow-x:hidden">
            {% block content %}{% endblock %} <!-- aside contents will have internal containers -->
          </div> <!-- #page-content -->
        </div>
      </div>
    </div>
  </div>
  <div class="infobar-wrapper scroll-pane">
    {% include rightbar %}
  </div>

  {% include demo-switcher %}
  {% include scripts %}

  <!-- Load page level scripts-->
  {% block scripts %}{% endblock %}
  <!-- End loading page level scripts-->
  <script type="text/javascript" src="assets/js/leftbar-mini.js"></script>
</body>
</html>