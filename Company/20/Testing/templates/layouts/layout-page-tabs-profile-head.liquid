{% include header %}
    <!-- The following CSS are included as plugins and can be removed if unused-->
    {% block styles %}{% endblock %}
    </head>

    <body class="infobar-offcanvas">
        {% include analytics %}
        {% include topnav %}

        <div id="wrapper">
            <div id="layout-static">
                <div class="static-leftbar-wrapper leftbar-bunting">
                    <div class="static-leftbar">
                        {% include leftbar %}
                    </div>
                </div>
                <div class="static-content-wrapper">
                    <div class="static-content">
                        <div class="page-content">
                            <div class="page-heading-bg-img clearfix">
                                <div class="page-heading mb-n">
                                    <div class="widget">
                                        <div class="widget-body">
                                            <div class="userinfo clearfix">
                                                <div class="avatar">
                                                    <img class="img-responsive img-circle" src="assets/demo/avatar/avatar_06.png">
                                                </div>
                                                <div class="info">
                                                    <span class="name">Jonathan Smith</span>
                                                    <span class="email">jon@tectonic.io</span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    {% include header-options-default %}
                                </div>
                                <div class="page-tabs">
                                    <ul class="nav nav-tabs material-nav-tabs">
                                        {% block tablist %}{% endblock %}
                                    </ul>
                                </div>
                            </div>
                            <div class="container-fluid">
                                {% block content %}{% endblock %}
                            </div> <!-- .container-fluid -->
                        </div> <!-- #page-content -->
                    </div>
                    {% include footer %}
                </div>
            </div>
        </div>


        <div class="infobar-wrapper scroll-pane">
            {% include rightbar %}
        </div>


    {% include demo-switcher %}
    {% include scripts %}
    
    <!-- Load page level scripts-->
    {% block scripts %}{% endblock %}
    <!-- End loading page level scripts-->

    </body>
</html>