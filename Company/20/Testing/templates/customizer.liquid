<script>
	var vars = {};
	vars['@leftbar-custom-bg-color'] = '#252f3d';
	vars['@leftbar-custom-color'] = '#5d6d82';
	vars['@leftbar-custom-link-color'] = '#98AFCC';
	vars['@leftbar-custom-link-hover-color'] = '#fff';
