	vars['@leftbar-custom-menu-icon-color'] = '#5d6d82';
	vars['@leftbar-custom-menu-hover-color'] = '#fff';
	vars['@leftbar-custom-menu-hover-bg-color'] = '#192029';
	vars['@leftbar-custom-menu-active-color'] = '#fff';
	vars['@leftbar-custom-menu-active-bg-color'] = '#2f3b4d';
	vars['@leftbar-custom-menu-border-color'] = '#e91e63';
	vars['@leftbar-custom-submenu-bg-color'] = '#192029';
	vars['@leftbar-custom-submenu-color'] = '#5d6d82';
	vars['@leftbar-custom-submenu-hover-color'] = '#fff';
	vars['@leftbar-custom-submenu-hover-bg-color'] = '#1f2833';
	vars['@leftbar-custom-muted-color'] = '#445263';
	vars['@leftbar-custom-custom-color1'] = '#11171f';
	vars['@leftbar-custom-custom-color2'] = '#11171f';
	vars['@leftbar-custom-custom-color3'] = '#11171f';
	vars['@leftbar-custom-custom-color4'] = '#11171f';

	vars['@topnav-custom-bg-color'] = '#192029';
	vars['@topnav-custom-color'] = '#5d6d82';
	vars['@topnav-custom-link-color'] = '#98AFCC';
	vars['@topnav-custom-link-hover-color'] = '#fff';
	vars['@topnav-custom-link-bg-color'] = '#252f3d';
	vars['@topnav-custom-link-hover-bg-color'] = '#2a3545';
	vars['@topnav-custom-link-active-color'] = '#fff';
	vars['@topnav-custom-link-active-bg-color'] = '#2a3545';
	vars['@topnav-custom-link-disabled-color'] = '#445263';

	$('.cpicker')
	.colorpicker()
	.on('changeColor.colorpicker', function (event) {
		var varVar = $(this).attr('data-var-name');
		console.log(varVar);
		vars['@'+varVar] = event.color.toHex();
		less.modifyVars(vars);
	});

	$(function () {
		setTimeout(function () {
			$('.static-leftbar-wrapper').addClass('leftbar-custom');
			$('#topnav').addClass('navbar-custom');
		}, 100);

		less.modifyVars(vars);
		less.refreshStyles();
	});

	$(".leftbar-bunting").css('border-right', 0);
	$(".leftbar-bunting .widget nav.widget-body > ul.acc-menu > li > a").css('border-right', 0);
</script>
